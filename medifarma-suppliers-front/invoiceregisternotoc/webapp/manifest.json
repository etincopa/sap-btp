{
	"_version": "3.0.0",
	"sap.app": {
		"id": "com.everis.suppliers.invoiceregisternotoc",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"crossNavigation": {
			"inbounds": {
				"intent1": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "semanticInvoiceRegisterNotOc",
					"action": "Display",
					"title": "{{appTitle}}",
					"icon": "sap-icon://request",
					"subTitle": "{{appDescription}}",
					"info": "{{appSubTitle}}"
				}
			}
		},
		"dataSources": {
			"SUPPLIERS_SRV": {
				"uri": "/saperp/sap/opu/odata/EPER/SUPPLIERS_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/SUPPLIERS_SRV/metadata.xml"
				}
			},
			"UTILS_SRV": {
				"uri": "/saperp/sap/opu/odata/EPER/UTILS_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/UTILS_SRV/metadata.xml"
				}
			},
			"MM_UTILS_SRV": {
				"uri": "/saperp/sap/opu/odata/EPER/OD_MM_UTILIDADES_SRV/",
				"type": "OData",
				"settings": {
					"localUri": "localService/OD_MM_UTILIDADES_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.platform.cf": {
	  "oAuthScopes": [
	    "$XSAPPNAME.invoiceregisternotoc"
	  ]
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "com.everis.suppliers.invoiceregisternotoc.view.Home",
			"type": "XML",
			"async": true,
			"id": "Home"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.everis.suppliers.invoiceregisternotoc.i18n.i18n"
				}
			},
			"SUPPLIERS_SRV": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false,
					"metadataUrlParams": {
						"sap-language": "ES"
					}
				},
				"dataSource": "SUPPLIERS_SRV",
				"preload": true
			},
			"UTILS_SRV": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false,
					"metadataUrlParams": {
						"sap-language": "ES"
					}
				},
				"dataSource": "UTILS_SRV",
				"preload": true
			},
			"MM_UTILS_SRV": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false,
					"metadataUrlParams": {
						"sap-language": "ES"
					}
				},
				"dataSource": "MM_UTILS_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.everis.suppliers.invoiceregisternotoc.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [{
				"name": "RouteHome",
				"pattern": "RouteHome",
				"target": ["TargetHome"]
			}],
			"targets": {
				"TargetHome": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewId": "Home",
					"viewName": "Home"
				}
			}
		}
	},
    "sap.cloud": {
      "public": true,
      "service": "medifarma-apps-suppliers-bs"
    }
}