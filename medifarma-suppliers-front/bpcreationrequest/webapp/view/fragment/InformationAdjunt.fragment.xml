<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:u="sap.ui.unified"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Panel expandable="false" headerText="{i18n>lblmandatorydocuments}" width="auto" class="sapUiResponsiveMargin">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:fields>
									<MessageStrip text="{i18n>msgformat}" showIcon="true" showCloseButton="true" class="sapUiMediumMarginBottom"></MessageStrip>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblsworndeclaration}" required="true" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader1" value="{UploadFile>/DJ}" name="myFileUpload1" uploadUrl="upload1/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="DJ" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="DJ" press="onDowloadFile" icon="sap-icon://download"/>
									<Button text="{i18n>btnformat}" width="50%" custom:custom="DJ" press="onDowloadFormat" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblrequestrucfile}" required="true" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader2" value="{UploadFile>/SR}" name="myFileUpload2" uploadUrl="upload2/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="SR" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="SR" press="onDowloadFile" icon="sap-icon://download"/>
									<Button text="{i18n>btnformat}" width="50%" custom:custom="SR" press="onDowloadFormat" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lbloperatinglicense}" required="{=${DataGeneral>/PersonType} === 1 ? false : true}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader3" value="{UploadFile>/EC}" name="myFileUpload3" uploadUrl="upload3/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="EC" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="EC" press="onDowloadFile" icon="sap-icon://download"/>
									<Button text="{i18n>btnformat}" width="50%" custom:custom="EC" press="onDowloadFormat" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
			</form:Form>
		</content>
	</Panel>
	<Panel expandable="true" headerText="{i18n>lbloptionaldocuments}" width="auto" class="sapUiResponsiveMargin">
		<content>
			<form:Form width="100%" editable="true" visible="true">
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblreferenceletter}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader4" value="{UploadFile>/CR}" name="myFileUpload4" uploadUrl="upload4/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="CR" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="CR" press="onDowloadFile" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblaffidavitofknowledge}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader5" value="{UploadFile>/DJC}" name="myFileUpload5" uploadUrl="upload5/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="DJC" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="DJC" press="onDowloadFile" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblidentificationdocument}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader6" value="{UploadFile>/DNI}" name="myFileUpload6" uploadUrl="upload6/" enabled="{general>/state}"
										tooltip="{i18n>tooltipidentificationdocument}" uploadComplete="handleUploadComplete" custom:custom="DNI" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="DNI" press="onDowloadFile" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblvalidityofpowers}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader7" value="{UploadFile>/VP}" name="myFileUpload7" uploadUrl="upload7/" enabled="{general>/state}"
										tooltip="{i18n>tooltipvalidityofpowers}" uploadComplete="handleUploadComplete" custom:custom="VP" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="VP" press="onDowloadFile" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblaffidavitpep}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader8" value="{UploadFile>/PEP}" name="myFileUpload8" uploadUrl="upload8/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="PEP" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="PEP" press="onDowloadFile" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblcertifications}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader9" value="{UploadFile>/CER}" name="myFileUpload9" uploadUrl="upload9/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="CER" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="CER" press="onDowloadFile" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<Label text="{i18n>lblothers}" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<u:FileUploader id="fileUploader10" value="{UploadFile>/OTR}" name="myFileUpload10" uploadUrl="upload10/" enabled="{general>/state}"
										tooltip="Upload your file to the local server" uploadComplete="handleUploadComplete" custom:custom="OTR" change="onSaveFile"
										buttonText="{i18n>btnFileUploaderText}" multiple="true" maximumFileSize="{config>/maxFileSizeForAttachments}" fileSizeExceed="onFileSizeExceed"/>
									<Button text="{i18n>btndownload}" width="50%" custom:custom="OTR" press="onDowloadFile" icon="sap-icon://download"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
					</form:FormContainer>
				</form:formContainers>
			</form:Form>
		</content>
	</Panel>
</core:FragmentDefinition>