{
    "_version": "3.0.0",
    "sap.app": {
        "id": "com.everis.ui5suppliersinvitation",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "dataSources": {
            "mainDataSource": {
                "uri": "/odata/v2/generate/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "SUPPLIERS_SRV": {
                "uri": "saperp/eper/SUPPLIERS_SRV/",
                "type": "OData",
                "settings": {
                    "odataVersion": "2.0"
                }
            },
            "SUPPLIERS_SRV_TEST": {
				"uri": "/saperp/sap/opu/odata/EPER/SUPPLIERS_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
        },
        "sourceTemplate": {
            "id": "html5moduletemplates.basicSAPUI5ApplicationProjectModule",
            "version": "1.40.12"
        },
        "crossNavigation": {
            "inbounds": {
                "intent1": {
                    "signature": {
                        "parameters": {},
                        "additionalParameters": "allowed"
                    },
                    "semanticObject": "suppliersInvitation",
                    "action": "display",
                    "title": "Invitaci√≥n de Proveedores",
                    "icon": "sap-icon://sales-document",
                    "subTitle": "",
                    "info": ""
                }
            }
        }
    },
    "sap.platform.cf": {
        "oAuthScopes": [
            "$XSAPPNAME.suppliersinvitation"
        ]
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": false,
        "rootView": {
            "viewName": "com.everis.ui5suppliersinvitation.view.Main",
            "type": "XML",
            "async": true
        },
        "dependencies": {
            "minUI5Version": "1.65.6",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {
                    "lazy": true
                }
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "com.everis.ui5suppliersinvitation.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainDataSource",
                "preload": true,
                "settings": {
                    "operationMode": "Server",
                    "groupId": "$auto",
                    "autoExpandSelect": true
                }
            },
            "SUPPLIERS_SRV": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false,
					"metadataUrlParams": {
						"sap-language": "ES"
					}
				},
				"dataSource": "SUPPLIERS_SRV",
				"preload": true
			},
            "SUPPLIERS_SRV_TEST": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Request",
					"useBatch": false
				},
				"dataSource": "SUPPLIERS_SRV_TEST",
				"preload": true
			},
            "invitation-model": {
                "type": "sap.ui.model.json.JSONModel",
                "preload": false
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "com.everis.ui5suppliersinvitation.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": true,
                "clearTarget": "true"
            },
            "routes": [
                {
                    "name": "RouteMain",
                    "pattern": "RouteMain",
                    "target": [
                        "TargetMain"
                    ]
                }
            ],
            "targets": {
                "TargetMain": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewName": "Main",
                    "viewId": "Main"
                }
            }
        }
    },
    "sap.cloud": {
      "public": true,
      "service": "medifarma-apps-suppliers-bs"
    }
}