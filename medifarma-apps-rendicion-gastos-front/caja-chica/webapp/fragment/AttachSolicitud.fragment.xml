<core:FragmentDefinition xmlns:core="sap.ui.core"
	xmlns="sap.m">
	<Dialog draggable="true" class="sapUiSizeCompact" title="Adjuntar Archivos" afterClose="onCloseDialogAttachSolicitud">
		<content>
			<UploadCollection noDataDescription="{i18n>txtnoDataDescription}" noDataText="{i18n>txtNoDataText}" maximumFilenameLength="{Config>/maximumFilenameLength}" maximumFileSize="{Config>/maximumFileSize}" multiple="{Config>/multiple}" busy="{Config>/busy}" change="onChangeSolicitud" typeMissmatch="onTypeMissmatch" fileType="{Config>/fileTypes}" uploadComplete="uploadComplete" uploadTerminated="onUploadTerminated" items="{ path: 'DocumentsSolicitud>/' }" uploadEnabled="{Config>/uploadEnabled}" mode="{Config>/mode}">
				<toolbar>
					<OverflowToolbar>
						<Title id="attachmentTitleSolicitud"/>
						<ToolbarSpacer/>
						<UploadCollectionToolbarPlaceholder/>
					</OverflowToolbar>
				</toolbar>
				<items>
					<UploadCollectionItem documentId="{DocumentsSolicitud>LoioId}" fileName="{DocumentsSolicitud>Descript}.{DocumentsSolicitud>DocType}" mimeType="{DocumentsSolicitud>Mimetype}" url="{DocumentsSolicitud>LoioId}" deletePress="onFileDeletedSolicitud" press=".onPressItemDownload" enableEdit="{Config>/enableEdit}" enableDelete="{Config>/enableDelete}" visibleDelete="{Config>/visibleDelete}" visibleEdit="{Config>/visibleEdit}">
					</UploadCollectionItem>
				</items>
			</UploadCollection>
		</content>
		<beginButton>
			<Button text="Cerrar" press="onCloseDialogAttachSolicitud" icon="sap-icon://decline" type="Default"/>
		</beginButton>
	</Dialog>
</core:FragmentDefinition>
