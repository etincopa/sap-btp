<core:FragmentDefinition xmlns="sap.suite.ui.commons" xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns:m="sap.m" height="auto">
	<m:Dialog id="idDialogo" contentWidth="auto" contentHeight="auto" class="sapUiContentPadding" >
		<m:content>
		<Timeline
		id="idTimeline"
		enableSocial="false"
		height="auto"
		width="auto"
		sort="false"
		showHeaderBar="false"
		content="{oModelTimeLine>/}"
		enableBusyIndicator = "true"
		enableScroll = "false"
		>
		<content>
				<TimelineItem
					id="idTemplateItem"
					maxCharacters="5000"
					dateTime="{oModelTimeLine>Date}"
					text="{oModelTimeLine>Text}"
					title="{oModelTimeLine>Title}"
					userName="{oModelTimeLine>User}"
					icon = "{oModelTimeLine>Icon}"
					status="{oModelTimeLine>Status}"
					userNameClickable = "true"
					userPicture="{Photo}"
					>
				</TimelineItem>
		</content>
		</Timeline>
		</m:content>
		<m:buttons>
			<m:Button text="Cerrar" press="handleCloseButton"/>
		</m:buttons>
	</m:Dialog>
</core:FragmentDefinition>
