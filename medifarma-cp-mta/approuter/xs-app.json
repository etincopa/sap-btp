{
    "authenticationMethod": "route",
    "logout": {
        "logoutEndpoint": "/do/logout"
    },
    "routes": [
        {
          "source": "^/srv_api/(.*)$",
          "target": "$1",
          "authenticationType": "none",
          "destination": "CP_SRV",
          "csrfProtection": false
        },
        {
            "source": "^/saperp/(.*)$",
            "target": "$1",
            "authenticationType": "none",
            "destination": "CP_S4H_HTTP_BASIC",
            "csrfProtection": false
        }
    ],
    "welcomeFile": "/cp.portal"
}