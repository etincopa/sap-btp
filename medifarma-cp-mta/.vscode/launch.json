{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds run",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/.vscode/cds",
      "args": [
        "run",
        "--with-mocks"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ],
      "console": "integratedTerminal",
      "autoAttachChildProcesses": true
    }
  ]
}