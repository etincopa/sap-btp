<mvc:View
	controllerName="com.medifarma.cp.manejopedidos.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:tnt="sap.tnt"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<tnt:ToolPage
		id="App"
		class="sapUiDemoToolPage"
	>
		<tnt:header>
			<tnt:ToolHeader>
				<Button
					id="sideNavigationToggleButton"
					icon="sap-icon://menu2"
					type="Transparent"
					press=".onSideNavButtonPress"
					tooltip="{i18n>collpaseMenuButtonText}"
				>
					<layoutData>
						<OverflowToolbarLayoutData priority="NeverOverflow"/>
					</layoutData>
				</Button>
				<ToolbarSpacer/>
				<Title
					text="{i18n>appTitle}"
					level="H2"
				/>
				<ToolbarSpacer/>
				<!-- <Button id="errorButton" icon="sap-icon://message-popup" visible="{= ${alerts>/alerts/errors}.length === 0 ? false : true }" type="Transparent" press=".onMessagePopoverPress" tooltip="{i18n>errorButtonTooltip}">
                    <layoutData>
                        <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
                    </layoutData>
                </Button>
                <Button id="notificationButton" icon="sap-icon://ui-notifications" visible="{= ${alerts>/alerts/notifications}.length === 0 ? false : true }" type="Transparent" press=".onNotificationPress" tooltip="{i18n>notificationButtonTooltip}">
                    <layoutData>
                        <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
                    </layoutData>
                </Button>
                <Button id="userButton" text="{i18n>userName}" type="Transparent" press=".onUserNamePress">
                    <layoutData>
                        <OverflowToolbarLayoutData closeOverflowOnInteraction="false"/>
                    </layoutData>
                </Button> -->
			</tnt:ToolHeader>
		</tnt:header>
		<tnt:sideContent>
			<tnt:SideNavigation
				expanded="true"
				selectedKey="{side>/selectedKey}"
			>
				<tnt:NavigationList items="{
					path: 'side>/navigation',
					templateShareable: false
				}">
					<!-- <tnt:NavigationListItem text="{path: 'side>titleI18nKey', formatter: '.getBundleText'}" icon="{side>icon}" expanded="{side>expanded}" items="{
							path: 'side>items',
							templateShareable: false
						}" key="{side>key}" href="#/{side>key}">
                        <tnt:NavigationListItem text="{path: 'side>titleI18nKey', formatter: '.getBundleText'}" key="{side>key}" href="#/{side>key}">
                        </tnt:NavigationListItem>
                    </tnt:NavigationListItem> -->
					<tnt:NavigationListItem
						text="{
							path: 'side>titleI18nKey',
							formatter: '.getBundleText'
						}"
						icon="{side>icon}"
						expanded="{side>expanded}"
						items="{
							path: 'side>items',
							templateShareable: false
						}"
						key="{side>key}"
						select="onPressMenuItem"
					>
						<tnt:NavigationListItem
							text="{
								path: 'side>titleI18nKey',
								formatter: '.getBundleText'
							}"
							key="{side>key}"
							app:route="{side>key}"
							app:prefixRol="{side>prefixRol}"
							select="onPressMenuItem"
						/>
					</tnt:NavigationListItem>
				</tnt:NavigationList>
				<tnt:fixedItem>
					<tnt:NavigationList items="{
						path: 'side>/fixedNavigation'
					}">
						<tnt:NavigationListItem
							text="{
								path: 'side>titleI18nKey',
								formatter: '.getBundleText'
							}"
							icon="{side>icon}"
							key="{side>key}"
							select="onPressFixMenuItem"
						/>
					</tnt:NavigationList>
				</tnt:fixedItem>
			</tnt:SideNavigation>
		</tnt:sideContent>
		<tnt:mainContents>
			<App id="app"/>
		</tnt:mainContents>
	</tnt:ToolPage>
</mvc:View>