<core:FragmentDefinition
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:com="sap.suite.ui.commons"
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:ui="sap.ui.table"
	xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<Dialog
		title="{i18n>tlHistorial}"
		contentWidth="100%"
		contentHeight="100%"
	>
		<content>
			<!--Label text="Historial" /-->
			<com:Timeline
				id="idTimeline"
				enableDoubleSided="true"
				groupBy="HireDate"
				growingThreshold="0"
				filterTitle="Estados"
				sortOldestFirst="false"
				content="{History>/}"
			>
				<com:content>
					<com:TimelineItem
						id="idTemplateItem"
						dateTime="{History>fechaRegistro}"
						title="{History>oEstadoAct/contenido}"
						userNameClickable="true"
						userPicture="sap-icon://customer"
						text="{History>descripcion}"
						userName="{History>usuario}"
						filterValue="{History>oEstadoAct/contenido}"
						icon="sap-icon://customer-history"
					/>
				</com:content>
			</com:Timeline>
		</content>
		<beginButton/>
		<buttons>
			<Button
				text="{i18n>btnClose}"
				type="Default"
				iconFirst="true"
				width="auto"
				press="onCloseDialogUA"
			/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>