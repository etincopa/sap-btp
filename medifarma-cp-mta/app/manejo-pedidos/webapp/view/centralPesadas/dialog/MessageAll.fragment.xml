<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:ui="sap.ui.table"
	xmlns:core="sap.ui.core"
	xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<Dialog
		id="idDialogMessageAll"
		title="Detalle"
		contentWidth="30%"
		contentHeight="50rem"
		draggable="true"
	>
		<content>
			<ui:Table
				id="idTableMessageAll"
				rows="{
					path: 'localModel>/Log'
				}"
				selectionMode="Single"
				visibleRowCount="10"
				enableSelectAll="false"
			>
				<ui:columns>
					<ui:Column
						width="4rem"
						hAlign="Left"
					>
						<Label text="Tipo"/>
						<ui:template>
							<Text text="{localModel>Type}"/>
						</ui:template>
					</ui:Column>
					<ui:Column
						width="auto"
						hAlign="Left"
					>
						<Label text="Mensaje"/>
						<ui:template>
							<HBox>
								<VBox class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom">
									<Label text="{localModel>Message}"/>
									<!--Text text="{localModel>PoItem}"/-->
								</VBox>
							</HBox>
						</ui:template>
					</ui:Column>
				</ui:columns>
			</ui:Table>
		</content>
		<beginButton/>
		<buttons>
			<Button
				text="Cerrar"
				type="Default"
				iconFirst="true"
				width="auto"
				press="onCloseDialog"
			/>
		</buttons>
		<customHeader/>
		<endButton/>
		<subHeader/>
	</Dialog>
</core:FragmentDefinition>