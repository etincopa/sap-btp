<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:ui="sap.ui.table"
	xmlns:l="sap.ui.layout"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<ui:Table
		id="idTablePedidos"
		rows="{
			path: '/VIEW_SEGUIMIENTO_SALA_CP',
			filters: [{
				path: 'tipoCodigo',
				operator: 'EQ',
				value1: 'PCP'
			}],
			sorter: [{
				path: 'numPedido',
				descending: true
			}, {
				path: 'numOrden',
				descending: false
			}, {
				path: 'numFraccion',
				descending: false
			}]
		}"
		rowSelectionChange="onRowSelectPedido"
		selectionMode="Single"
		visibleRowCount="5"
		enableSelectAll="false"
	>
		<ui:extension>
			<OverflowToolbar style="Clear">
				<ToolbarSpacer/>
				<Button
					icon="sap-icon://cancel"
					tooltip="{i18n>tpAnular}"
					type="Reject"
					text="{i18n>btnAnular}"
					press="onAnularPedido"
				/>
				<ToolbarSeparator/>
				<OverflowToolbarButton
					text="{i18n>btnExport}"
					tooltip="{i18n>btnExport}"
					icon="sap-icon://excel-attachment"
					press="onExportXLSReport"
				/>
			</OverflowToolbar>
		</ui:extension>
		<ui:columns>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>lblPedido}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numPedido}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="10rem"
				hAlign="Begin"
			>
				<Text
					text="{i18n>clTipoPedido}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{tipoPedido}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clOrdReser}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numOrden}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="4rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFraccion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numFraccion}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="4.5rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clImprimir}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Button
						icon="sap-icon://print"
						type="{= ${codEtiquetaOrden} ? 'Emphasized' : 'Default'}"
						app:level="ORDEN"
						press="onImprimir"
						visible="{= ${codEstadoOrden} === 'PAOPEFI' || ${codEstadoOrden} === 'PAOENFI' || ${codEstadoOrden} === 'PAOPARC' || ${codEstadoOrden} === 'PAOTOT' ? true : false }"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clCodPT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{codProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="18rem"
				hAlign="Begin"
			>
				<Text
					text="{i18n>clDescPT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{nomProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clLotePT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{loteProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clProgramar} / {i18n>clDesprogramar}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<HBox>
						<Button
							icon="sap-icon://share-2"
							tooltip="{i18n>clProgramar}"
							app:progAction="PROGRAMAR"
							enabled="{= ${codEstadoOrden} === 'PAOATEN' || ${codEstadoOrden} === 'PAOAPAR' ? true : false }"
							press="onProgramarAction"
						/>
						<Button
							icon="sap-icon://cancel-share"
							type="Reject"
							tooltip="{i18n>clDesprogramar}"
							app:progAction="DESPROGRAMAR"
							enabled="{= ${codEstadoOrden} === 'PAOPRSA' ? true : false }"
							press="onProgramarAction"
						/>
					</HBox>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="14rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>dclSala}"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Input
						value="{sala}"
						placeholder="{i18n>dclSala}"
						type="Text"
						showValueHelp="true"
						visible="true"
						width="14rem"
						valueHelpOnly="true"
						valueHelpRequest=".onMatchCodeSala"
						app:level="ORDEN"
						enabled="{= ${codEstadoOrden} === 'PAOPRSA' || ${codEstadoOrden} === 'PAOATEN' || ${codEstadoOrden} === 'PAOAPAR' ? true : false }"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="14rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clEstado}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<ObjectStatus
						text="{estadoOrden}"
						state="{
							path: 'codEstadoOrden',
							formatter: '.formatter.highlightStatusOrden'
						}"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuProgramSala}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgPurple"
				/>
				<ui:template>
					<Text
						text="{programacionSalaUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecProgramSala}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgPurple"
				/>
				<ui:template>
					<Text
						text="{
							path: 'programacionSalaFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraProgramSala}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgPurple"
				/>
				<ui:template>
					<Text
						text="{
							path: 'programacionSalaFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{usuCreacion}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecCreacion',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecCreacion',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuIniPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{pesajeIniUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecIniPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeIniFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraIniPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeIniFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuFinPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{pesajeFinUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecFinPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeFinFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraFinPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeFinFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
		</ui:columns>
	</ui:Table>
	<l:ResponsiveSplitter
		defaultPane="default"
		height="auto"
	>
		<l:PaneContainer>
			<l:SplitPane
				requiredParentWidth="700"
				id="default"
			>
				<l:layoutData>
					<l:SplitterLayoutData size="auto"/>
				</l:layoutData>
				<ui:Table
					id="idTableInsumos"
					rows="{
						path: 'FraccionDetalleModel>/Insumos',
						sorter: [{
                            path: 'ordenNumero',
                            descending: false
                        }, {
                            path: 'ordenPos',
                            descending: false
                        }, {
                            path: 'numFraccion',
                            descending: false
                        }, {
                            path: 'numSubFraccion',
                            descending: false
                        }, {
                            path: 'codigoInsumo',
                            descending: false
                        }, {
                            path: 'lote',
                            descending: false
                        }]
					}"
					rowSelectionChange="onRowSelectInsumo"
					selectionMode="Single"
					visibleRowCount="6"
					enableSelectAll="false"
				>
					<ui:columns>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clReset}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<HBox>
									<Button
										icon="sap-icon://reset"
										type="Reject"
										tooltip="{i18n>clReset}"
										enabled="{= (${FraccionDetalleModel>insumoEstado} === 'PAIPEFI' || ${FraccionDetalleModel>insumoEstado} === 'PAIPARC') &amp;&amp; ${FraccionDetalleModel>bCantPesar} === true ? true : false }"
										press="onResetAction"
									/>
								</HBox>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="14rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>dclSala}"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Input
									value="{FraccionDetalleModel>insumoSala}"
									placeholder="{i18n>dclSala}"
									type="Text"
									showValueHelp="true"
									visible="true"
									width="14rem"
									valueHelpOnly="true"
									valueHelpRequest=".onMatchCodeSala"
									app:level="INSUMO"
									enabled="{= ${FraccionDetalleModel>insumoEstado} === 'PAIPEPE' ? true : false }"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text text="{i18n>clPosnr}"/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>ordenPos}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clSubFraccion}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>numSubFraccion}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>lblCodigoInsumo}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>codigoInsumo}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>lblLoteInsumo}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>lote}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="16rem"
							hAlign="Begin"
						>
							<Text
								text="{i18n>clDescripcion}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>descripcion}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clPotenciaTeorica}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>pTInsumo',
										formatter: '.formatter.formatWeightString'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clPotenciaPractica}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>pPLoteLog',
										formatter: '.formatter.formatWeightString'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clCantReq}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>cantRequerida',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="3rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clUnidad}"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>cantRequeridaUM}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>dclCantSugTotal}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>cantSugeridaTotal',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>dclCantSug}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>cantSugerida',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clCantPed}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
