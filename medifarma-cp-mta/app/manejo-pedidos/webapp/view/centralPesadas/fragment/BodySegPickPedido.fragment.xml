<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:ui="sap.ui.table"
	xmlns:l="sap.ui.layout"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<ui:Table
		id="idTablePedidos"
		rows="{
			path: '/VIEW_SEGUIMIENTO_PICKING_CP',
			filters: [{
				path: 'tipoCodigo',
				operator: 'EQ',
				value1: 'PCP'
			}],
			sorter: [{
				path: 'numPedido',
				descending: true
			}, {
				path: 'numOrden',
				descending: false
			}, {
				path: 'numFraccion',
				descending: false
			}]
		}"
		threshold="100"
		enableBusyIndicator="true"
		rowSelectionChange="onRowSelectPedido"
		selectionMode="Single"
		visibleRowCount="5"
		enableSelectAll="false"
	>
		<ui:extension>
			<OverflowToolbar style="Clear">
				<Title text="{i18n>lblPedido}"/>
				<ToolbarSpacer/>
				<ToolbarSeparator/>
				<Button
					icon="sap-icon://cancel"
					tooltip="{i18n>tpAnular}"
					type="Reject"
					text="{i18n>btnAnular}"
					press="onAnularPedido"
				/>
				<OverflowToolbarButton
					text="{i18n>btnExport}"
					tooltip="{i18n>btnExport}"
					icon="sap-icon://excel-attachment"
					press="onExportXLSReport"
				/>
			</OverflowToolbar>
		</ui:extension>
		<ui:columns>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>lblPedido}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numPedido}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="10rem"
				hAlign="Begin"
			>
				<Text
					text="{i18n>clTipoPedido}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{tipoPedido}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clOrdReser}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numOrden}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="4rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFraccion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numFraccion}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clCodPT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{codProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="18rem"
				hAlign="Begin"
			>
				<Text
					text="{i18n>clDescPT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{nomProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clLotePT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{loteProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="14rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>dclSala}"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{sala}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="14rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clEstado}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<ObjectStatus
						text="{estadoOrden}"
						state="{
							path: 'codEstadoOrden',
							formatter: '.formatter.highlightStatusOrden'
						}"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>lblTamanoLote}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{tamanoLote}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="5rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clCantItems}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{cantInsumos}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clCantBultos}"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{cantBultos}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{usuCreacion}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecCreacion',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecCreacion',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuIniPicking}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{usuInicioPick}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecIniPicking}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecInicioPick',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraIniPicking}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecInicioPick',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuFinPicking}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{usuFinPick}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecFinPicking}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecFinPick',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraFinPicking}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecFinPick',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
		</ui:columns>
	</ui:Table>
	<l:ResponsiveSplitter
		defaultPane="default"
		height="auto"
	>
		<l:PaneContainer>
			<l:SplitPane
				requiredParentWidth="700"
				id="default"
			>
				<l:layoutData>
					<l:SplitterLayoutData size="auto"/>
				</l:layoutData>
				<ui:Table
					id="idTableInsumos"
					rows="{
						path: 'FraccionDetalleModel>/Insumos',
						sorter: [{
                            path: 'ordenNumero',
                            descending: false
                        }, {
                            path: 'ordenPos',
                            descending: false
                        }, {
                            path: 'numFraccion',
                            descending: false
                        }, {
                            path: 'numSubFraccion',
                            descending: false
                        }, {
                            path: 'codigoInsumo',
                            descending: false
                        }, {
                            path: 'lote',
                            descending: false
                        }]
					}"
					rowSelectionChange="onRowSelectInsumo"
					selectionMode="Single"
					visibleRowCount="6"
					enableSelectAll="false"
				>
					<ui:extension>
						<OverflowToolbar style="Clear">
							<ToolbarSpacer/>
							<ToolbarSeparator/>
							<OverflowToolbarButton
								text="{i18n>btnExport}"
								tooltip="{i18n>btnExport}"
								icon="sap-icon://excel-attachment"
								press="onExportXLSInsumoReport"
							/>
						</OverflowToolbar>
					</ui:extension>
					<ui:columns>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clLiberar}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Button
									icon="sap-icon://paper-plane"
									type="Default"
									press="onLiberarInsumo"
									visible="{= ${FraccionDetalleModel>insumoEstado} === 'PAIATPI' ? true : false}"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>dclAlmacen}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>almacen}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text text="{i18n>clPosnr}"/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>ordenPos}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clSubFraccion}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>numSubFraccion}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>lblCodigoInsumo}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>codigoInsumo}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>lblLoteInsumo}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>lote}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="18rem"
							hAlign="Begin"
						>
							<Text
								text="{i18n>clDescripcion}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>descripcion}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="14rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clEstado}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<ObjectStatus
									text="{FraccionDetalleModel>insumoEstadoDes}"
									state="{
										path: 'FraccionDetalleModel>insumoEstado',
										formatter: '.formatter.highlightStatusInsumo'
									}"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clCantReq}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>cantRequerida',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="3rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clUnidad}"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>cantRequeridaUM}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>dclCantSugTotal}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>cantSugeridaTotal',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>dclCantSug}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>cantSugerida',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clCantPed}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>sugerido',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
                        <ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text text="{i18n>dclAgotar}"/>
							<ui:template>
								<Text
									text="{= ${FraccionDetalleModel>agotar} === '' ? 'No': 'Si'}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clCantEntero}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>cantAtendida',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="3rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clUnidad}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>unidad}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
					</ui:columns>
				</ui:Table>
			</l:SplitPane>
			<l:SplitPane>
				<l:layoutData>
					<l:SplitterLayoutData size="40%"/>
				</l:layoutData>
				<ui:Table
					rows="{
						path: 'FraccionDetalleModel>/Bultos',
						sorter: [{
							path: 'Tipo',
							descending: false
						}]
					}"
					id="idTableBultos"
					selectionMode="None"
					visibleRowCount="6"
					enableSelectAll="false"
					alternateRowColors="true"
				>
					<ui:rowSettingsTemplate>
						<!--ui:RowSettings highlight="{= ${FraccionDetalleModel>Tipo} === 'SALDO' ? 'Indication07' : 'Indication01'}"/-->
					</ui:rowSettingsTemplate>
					<ui:columns>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clAccionTarar}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Button
									app:idBulto="{FraccionDetalleModel>IdBulto}"
									icon="sap-icon://compare-2"
									press="onTararEnteroPress"
									visible="{= ${FraccionDetalleModel>Tipo} === 'ENTERO' ? true : false}"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clCodigoBulto}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>IdBulto}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="8rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clTicketAten}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>Etiqueta}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clTipoBulto}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<ObjectStatus
									text="{FraccionDetalleModel>Tipo}"
									state="{
										path: 'FraccionDetalleModel>Tipo',
										formatter: '.formatter.highlightTipoBultoInsumo'
									}"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clPesoNeto}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>CantidadA',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clTara}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>Tara',
										formatter: '.formatter.formatWeight'
									}"
									wrapping="false"
									visible="{= ${FraccionDetalleModel>UnidadM} === 'KG' || ${FraccionDetalleModel>UnidadM} === 'G' || ${FraccionDetalleModel>UnidadM} === 'kg' || ${FraccionDetalleModel>UnidadM} === 'g' ? true : false}"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clPesoBruto}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="
										{
										parts: [{
											path: 'FraccionDetalleModel>CantidadA'
										}, {
											path: 'FraccionDetalleModel>Tara'
										}, {
											path: '0'
										}],
										formatter: '.formatter.getSuma'
									}"
									wrapping="false"
									visible="{= ${FraccionDetalleModel>UnidadM} === 'KG' || ${FraccionDetalleModel>UnidadM} === 'G' || ${FraccionDetalleModel>UnidadM} === 'kg' || ${FraccionDetalleModel>UnidadM} === 'g' ? true : false}"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="3rem"
							hAlign="End"
						>
							<Text
								text="{i18n>clUnidad}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{= ${FraccionDetalleModel>Tipo} === 'FRACCION' ? ${FraccionDetalleModel>BalanzaUnidadM} : ${FraccionDetalleModel>UnidadM}}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clUsuRegistro}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>UsuarioAte}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clFecRegistro}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>FechaAte',
										formatter: '.formatter.getTimestampToDMY2'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="6rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clHorRegistro}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{
										path: 'FraccionDetalleModel>HoraAte',
										formatter: '.formatter.getMsToHMS'
									}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clImprimir}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Button
									icon="sap-icon://print"
									type="Emphasized"
									press="onImprimir"
									visible="{= ${FraccionDetalleModel>Tipo} === 'ENTERO' ? true : false}"
								/>
							</ui:template>
						</ui:Column>
					</ui:columns>
				</ui:Table>
			</l:SplitPane>
			<!-- <l:PaneContainer resize=".onInnerContainerResize">

            </l:PaneContainer> -->
		</l:PaneContainer>
	</l:ResponsiveSplitter>
</core:FragmentDefinition>