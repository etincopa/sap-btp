<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:ui="sap.ui.table"
	xmlns:l="sap.ui.layout"
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
>
	<ui:Table
		id="idTablePedidos"
		rows="{
			path: '/VIEW_SEGUIMIENTO_TRASLADO_CP',
			filters: [{
				path: 'tipoCodigo',
				operator: 'EQ',
				value1: 'PCP'
			}],
			sorter: [{
				path: 'numPedido',
				descending: true
			}, {
				path: 'numOrden',
				descending: false
			}, {
				path: 'numFraccion',
				descending: false
			}]
		}"
		threshold="100"
		enableBusyIndicator="true"
		selectionMode="Single"
		rowSelectionChange="onRowSelectPedido"
		visibleRowCount="5"
		enableSelectAll="false"
	>
		<ui:extension>
			<OverflowToolbar style="Clear">
				<ToolbarSpacer/>
				<Button
					id="messagePopoverBtn"
					icon="sap-icon://information"
					type="Default"
					text=""
					press=".handleMessagePopoverPress"
					visible="false"
				/>
				<ToolbarSeparator/>
				<OverflowToolbarButton
					text="{i18n>btnExport}"
					tooltip="{i18n>btnExport}"
					icon="sap-icon://excel-attachment"
					press="onExportXLSReport"
				/>
			</OverflowToolbar>
		</ui:extension>
		<ui:columns>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>lblPedido}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numPedido}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="10rem"
				hAlign="Begin"
			>
				<Text
					text="{i18n>clTipoPedido}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{tipoPedido}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clOrdReser}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numOrden}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="4rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFraccion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{numFraccion}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="4rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>dclCentro}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{centro}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clCodPT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{codProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="18rem"
				hAlign="Begin"
			>
				<Text
					text="{i18n>clDescPT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{nomProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clLotePT}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{loteProdTerm}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="14rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>dclSala}"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<ComboBox
                        width="14rem"
						placeholder="{i18n>dclSala}"
						selectedKey="{salaPesajeId}"
						items="{
							path: 'SalaCentroModel>/',
							sorter: [{
								path: 'oPlanta/codigo',
								descending: false
							}, {
								path: 'sala',
								descending: false
							}]
						}"
						editable="false"
					>
						<core:Item
							key="{SalaCentroModel>salaPesajeId}"
							text="{SalaCentroModel>sala} - {SalaCentroModel>oPlanta/contenido}"
						/>
					</ComboBox>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="4.5rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clTraslado}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Button
						icon="sap-icon://shipping-status"
						tooltip="{i18n>clTraslado}"
						type="Default"
						press="onTraslado"
						enabled="{= ${codEstadoOrden} === 'PAOPEFI' &amp;&amp; ${ordenVerificacionEstado} === 'V' ? true : false}"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="9rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>dclPedidoTraslado}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{pedidoTraslado}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="14rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clEstado}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<ObjectStatus
						text="{estadoOrden}"
						state="{
							path: 'codEstadoOrden',
							formatter: '.formatter.highlightStatusOrden'
						}"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>dclVerificado}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<ObjectStatus
						text="{
							path: 'ordenVerificacionEstado',
							formatter: '.formatter.textStatusVerif'
						}"
						state="{
							path: 'ordenVerificacionEstado',
							formatter: '.formatter.highlightStatusVerif'
						}"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>lblTamanoLote}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{tamanoLote}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuTransferencia}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{pedidoTrasladoUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecTransferencia}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pedidoTrasladoFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraTransferencia}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pedidoTrasladoFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuIniVerif}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgLBlue"
				/>
				<ui:template>
					<Text
						text="{verificacionIniUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecIniVerif}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgLBlue"
				/>
				<ui:template>
					<Text
						text="{
							path: 'verificacionIniFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraIniVerif}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgLBlue"
				/>
				<ui:template>
					<Text
						text="{
							path: 'verificacionIniFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuFinVerif}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgPurple"
				/>
				<ui:template>
					<Text
						text="{verificacionFinUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecFinVerif}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgPurple"
				/>
				<ui:template>
					<Text
						text="{
							path: 'verificacionFinFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraFinVerif}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgPurple"
				/>
				<ui:template>
					<Text
						text="{
							path: 'verificacionFinFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuEntregaFis}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{entregaFisUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecEntregaFis}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'entregaFisFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraEntregaFis}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'entregaFisFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{usuCreacion}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecCreacion',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraCreacion}"
					width="auto"
					maxLines="2"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
					class="bgGreen"
				/>
				<ui:template>
					<Text
						text="{
							path: 'fecCreacion',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuIniPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{pesajeIniUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecIniPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeIniFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraIniPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeIniFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="8rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clUsuFinPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{pesajeFinUsu}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="7rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clFecFinPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeFinFec',
							formatter: '.formatter.getTimestampToDMY'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
			<ui:Column
				width="6rem"
				hAlign="Center"
			>
				<Text
					text="{i18n>clHoraFinPesaje}"
					width="auto"
					maxLines="3"
					wrapping="true"
					textAlign="Center"
					textDirection="Inherit"
				/>
				<ui:template>
					<Text
						text="{
							path: 'pesajeFinFec',
							formatter: '.formatter.getTimestampToHMS'
						}"
						wrapping="false"
					/>
				</ui:template>
			</ui:Column>
		</ui:columns>
	</ui:Table>
	<l:ResponsiveSplitter
		defaultPane="default"
		height="auto"
	>
		<l:PaneContainer>
			<l:SplitPane
				requiredParentWidth="700"
				id="default"
			>
				<l:layoutData>
					<l:SplitterLayoutData size="auto"/>
				</l:layoutData>
				<ui:Table
					id="idTableInsumos"
					rows="{
						path: 'FraccionDetalleModel>/Insumos',
						sorter: [{
                            path: 'ordenNumero',
                            descending: false
                        }, {
                            path: 'ordenPos',
                            descending: false
                        }, {
                            path: 'numFraccion',
                            descending: false
                        }, {
                            path: 'numSubFraccion',
                            descending: false
                        }, {
                            path: 'codigoInsumo',
                            descending: false
                        }, {
                            path: 'lote',
                            descending: false
                        }]
					}"
					rowSelectionChange="onRowSelectInsumo"
					selectionMode="Single"
					visibleRowCount="6"
					enableSelectAll="false"
				>
					<ui:columns>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text text="{i18n>clPosnr}"/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>ordenPos}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="4rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>clSubFraccion}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>numSubFraccion}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="7rem"
							hAlign="Center"
						>
							<Text
								text="{i18n>lblCodigoInsumo}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
								<Text
									text="{FraccionDetalleModel>codigoInsumo}"
									wrapping="false"
								/>
							</ui:template>
						</ui:Column>
						<ui:Column
							width="18rem"
							hAlign="Begin"
						>
							<Text
								text="{i18n>clDescripcion}"
								width="auto"
								maxLines="2"
								wrapping="true"
								textAlign="Center"
								textDirection="Inherit"
							/>
							<ui:template>
