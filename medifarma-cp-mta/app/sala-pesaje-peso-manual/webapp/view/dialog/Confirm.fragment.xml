<core:FragmentDefinition xmlns="sap.m"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core"

    xmlns:custom="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
    <Dialog class="sapUiSizeCompact" contentWidth="400px">
        <content>

            <f:SimpleForm id="SimpleFormDisplay354"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="4"
                adjustLabelSpan="false"
                emptySpanXL="4"
                emptySpanL="4"
                emptySpanM="4"
                emptySpanS="0"
                columnsXL="2"
                columnsL="2"
                columnsM="2"
                singleContainerFullSize="false" >
                <f:content>
                    <core:Title text="{i18n>tlDatosConfirmacion}" />
                    <Label text="{i18n>lblUsuario}"/>
                    <Input type="Text" value="{localModel>/Usuario}" enabled="false" showValueHelp="false" visible="true" width="100%" valueHelpOnly="false"/>
                    <Label text="{i18n>lblContrasenia}"/>
                    <Input type="Password" value="{localModel>/Contrasenia}" showValueHelp="false" visible="true" width="100%" valueHelpOnly="false"/>
                    
                </f:content>
            </f:SimpleForm>

        </content>
        <beginButton/>
        <buttons>
            <Button text="{i18n>btnGuardar}" type="Default" iconFirst="true" width="auto" press="onPressSave"/>
            <Button text="{i18n>btnCancelar}" type="Default" iconFirst="true" width="auto" press="onPressCancel"/>
        </buttons>
        <customHeader/>
        <endButton/>
        <subHeader/>
    </Dialog>
</core:FragmentDefinition>