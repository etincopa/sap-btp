<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Table id="idTblEquipment" width="auto" sticky="ColumnHeaders" mode="MultiSelect" items="{aListEquipoUtensilio>/}" updateFinished="onUpdateFinish" growing="true"
		growingScrollToLoad="true">
		<headerToolbar>
			<OverflowToolbar>
				<Title text="Equipos ({=${aListEquipoUtensilio>/}.length})"></Title>
				<ToolbarSpacer/>
				<ToolbarSeparator/>
				<OverflowToolbarButton text="{i18n>btnAgregarAgrupador}" tooltip="{i18n>btnAgregarAgrupador}" icon="sap-icon://add" press="onAddAgrupador"/>
			</OverflowToolbar>
		</headerToolbar>
		<columns>
			<Column width="20%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
				<Text text="{i18n>txtCode}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
			</Column>
			<Column width="35%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
				<Text text="{i18n>txtDescription}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
			</Column>
			<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
				<Text text="{i18n>txtType}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
			</Column>
			<!-- <Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
				<Text text="{i18n>clCalification}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
			</Column> -->
			<Column width="25%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
				<Text text="{i18n>txtArea}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
			</Column>
			<Column width="10%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
				<Text text="{i18n>txtPrincipal}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
			</Column>
		</columns>
		<items>
			<ColumnListItem>
				<Text text="{aListEquipoUtensilio>equnr}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				<Text text="{aListEquipoUtensilio>eqktx}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				<Text text="{aListEquipoUtensilio>tipo}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				<!-- <ObjectStatus text="{aListEquipoUtensilio>dateCalibracion}" state="{aListEquipoUtensilio>stateCalibracion}"/> -->
				<Text text="{aListEquipoUtensilio>pltxt}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				<CheckBox selected="{= ${aListEquipoUtensilio>ctext}===''?false:true  }" enabled="false" />
			</ColumnListItem>
		</items>
	</Table>
</core:FragmentDefinition>