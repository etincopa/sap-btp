<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:dnd="sap.ui.core.dnd">
	<!-- <Dialog title="{asociarDatos>/codigo} - {asociarDatos>/descripcion}" afterClose="onCancelCuadroResponsive"> -->
		<ScrollContainer
		height="100%"
		width="100%"
		horizontal="{= !${device>/system/desktop} }"
		vertical="true">
		<VBox>
			<Table width="auto" sticky="HeaderToolbar,ColumnHeaders" mode="None" items="{aListInsumosAssignResponsive>/}" updateFinished="onUpdateFinish" growing="true"
				growingScrollToLoad="true">
				<headerToolbar>
					<OverflowToolbar>
						<!-- <Title text="{i18n>tltEquipos} ({=${aListEquipoAssignResponsive>/}.length})"></Title> -->
						<ToolbarSpacer/>
						<ToolbarSeparator/>
						<Button icon="sap-icon://refresh" tooltip="{i18n>btnRefreshInsumos}" press="onRefreshInsumosSAP" type="Emphasized" enabled="{modelGeneral>/refreshHabilitar}"/>
						<Button icon="sap-icon://multiselect-all" tooltip="{i18n>btnListInsumos}" press="onListInsumosCP" type="Emphasized" enabled="{aListInsumosAssignResponsive>/0/generalEnabledSaveButton}"/>
					</OverflowToolbar>
				</headerToolbar>
				<columns>
					<Column width="{= ${device>/system/desktop} ? '20%' : '200px' }" hAlign="Left" vAlign="Middle" styleClass="cellBorderLeft cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clDescription}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '10%' : '100px' }" hAlign="Left" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clCodigo}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '8%' : '80px' }" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clAdicional}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '5%' : '50px' }" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clUME}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '10%' : '100px' }" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clCount}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '10%' : '100px' }" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clCantidadOP}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '10%' : '100px' }" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clUMB}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '10%' : '100px' }" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clLote}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '10%' : '120px' }" hAlign="End" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clCountGive}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '10%' : '100px' }" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clBulto}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '12%' : '120px' }" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="{i18n>clUsuarioVerificador}" width="auto"/>
					</Column>
					<Column width="{= ${device>/system/desktop} ? '5%' : '50px' }" hAlign="Center" vAlign="Middle" styleClass="cellBorderRight" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<Text text="" width="auto"/>
					</Column>
				</columns>
				<items>
					<ColumnListItem>
						<Text text="{aListInsumosAssignResponsive>Maktx}" width="auto"/>
						<Text text="{aListInsumosAssignResponsive>Component}" width="auto"/>
						<Text text="{=${aListInsumosAssignResponsive>adicional} ? 'S' : 'N'}" width="auto"/>
						<Text text="{aListInsumosAssignResponsive>CompUnit}" width="auto"/>
						<Text text="{aListInsumosAssignResponsive>CompQty}" width="auto"/>
						<Text text="{aListInsumosAssignResponsive>cantidadOP}" width="auto"/>
						<Text text="{aListInsumosAssignResponsive>umb}" width="auto"/>
						<Text text="{=${aListInsumosAssignResponsive>Mtart} === 'ZSEB' ? ${aListInsumosAssignResponsive>Charg} : '---'}" width="auto"/>
						<Input value="{aListInsumosAssignResponsive>cantidadBarCode}" liveChange="onSaveLineaActual" enabled="{=${aListInsumosAssignResponsive>bEditableCant} === true &amp;&amp; ${generalModelState>/state} === true ? true : false}" width="auto"/>
						<Input value="{aListInsumosAssignResponsive>numeroBultos}" enabled="{=${aListInsumosAssignResponsive>bEnabledInputBulto} === true &amp;&amp; ${generalModelState>/state} === true ? true : false}" width="auto"/>
						<Text text="{aListInsumosAssignResponsive>usuarioVerificador}" width="auto"/>
						<VBox>
							<CheckBox selected="{aListInsumosAssignResponsive>verifCheck}" select="onVerificarManual" enabled="{generalModelState>/state}"/>	
						</VBox>
					</ColumnListItem>
				</items>
			</Table>
			<Table width="auto" mode="None" updateFinished="onUpdateFinish" growing="true"
				growingScrollToLoad="true" noDataText=" ">
				<columns>
					<Column styleClass="cellBorderLeft cellBorderRight"></Column>
					<Column styleClass="cellBorderLeft cellBorderRight"></Column> 
				</columns>
				<items>
					<ColumnListItem>
						<Label text="{i18n>ftTVerificado}{aFooter>/usuario}" design="Bold" width="auto"/>
						<Label text="{i18n>ftTFecha}{aFooter>/fecha}" design="Bold" width="auto"/>
					</ColumnListItem>
				</items>
			</Table>
		</VBox>
	</ScrollContainer>
		<!-- <endButton>
			<Button text="{i18n>btnClose}" press="onCancelCuadroResponsive" type="Transparent"/>
		</endButton>
	</Dialog> -->
	</core:FragmentDefinition>