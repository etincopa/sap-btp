<mvc:View
    controllerName="mif.rmd.registro.controller.MainView"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
  <App id="app">
    <pages>
      <Page id="page" title="{i18n>title}">
        <content>
          <core:Fragment fragmentName="mif.rmd.registro.view.fragment.master.Filter" type="XML"/>
          <core:Fragment fragmentName="mif.rmd.registro.view.fragment.master.Body" type="XML"/>
        </content>
      </Page>
    </pages>
  </App>
</mvc:View>
