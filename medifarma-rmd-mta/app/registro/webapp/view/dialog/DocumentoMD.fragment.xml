<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<ScrollContainer width="100%" height="80%" horizontal="true" vertical="true" focusable="true">
		<Table items="{modelGeneral>/Adjuntos}">
            <columns>
                <Column width="3em">
                    <Text text="" />
                </Column>
                <Column>
                    <Text text="Nombre" />
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <core:Icon src="sap-icon://pdf-attachment"/>
                        <Link text="{modelGeneral>Name}" press="onDownloadAdjuntoMD"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
	</ScrollContainer>
</core:FragmentDefinition>