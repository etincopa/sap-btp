<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:dnd="sap.ui.core.dnd">
	<Dialog title="{asociarDatos>/codigo} - {asociarDatos>/descripcion}" afterClose="onCancelEtiquetaResponsive">
		<Table width="auto" id="idTblEtiqueta" sticky="HeaderToolbar,ColumnHeaders" mode="MultiSelect" items="{listMdEsEtiquetaGeneral>/}" updateFinished="onUpdateFinish">
			<dragDropConfig>
				<dnd:DragDropInfo
                    sourceAggregation="items"
                    targetAggregation="items"
                    dropPosition="Between"
                    dropEffect="Move"
                    drop="onDropResponsive"/>
			</dragDropConfig>
			<headerToolbar>
				<OverflowToolbar>
					<Title text="{i18n>addEtiqueta} ({=${listMdEsEtiquetaGeneral>/}.length})"></Title>
					<ToolbarSpacer/>
					<ToolbarSeparator/>
					<OverflowToolbarButton text="{i18n>TbEPrint}" tooltip="{i18n>TbEPrint}" icon="sap-icon://print" press="onPrint" type="Transparent"/>
					<OverflowToolbarButton text="{i18n>addEtiqueta}" tooltip="{i18n>addEtiqueta}" icon="sap-icon://add" press="onAddEtiquetasResponsive" type="Transparent" enabled="{=${asociarDatos>/estadoIdRmd_iMaestraId}=== 465 ? false : true}"/>
					<OverflowToolbarButton text="{i18n>btnSave}" tooltip="{i18n>btnSave}" icon="sap-icon://save" press="onSaveEtiquetasResponsive" type="Transparent" enabled="{=${asociarDatos>/estadoIdRmd_iMaestraId}=== 465 ? false : true}"/>
					<OverflowToolbarButton text="{i18n>btnDelete}" tooltip="{i18n>btnDelete}" icon="sap-icon://delete" press="onDeleteEtiquetasResponsive" type="Transparent" enabled="{=${asociarDatos>/estadoIdRmd_iMaestraId}=== 465 ? false : true}"/>
				</OverflowToolbar>
			</headerToolbar>
			<columns>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>tbEOrden}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbCCodigo}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="25%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbEtDescripcion}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbEtItems}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbEtConforme}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbEtProccessMenor}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbEtAcc}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<Text text="{listMdEsEtiquetaGeneral>orden}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
					<Text text="{listMdEsEtiquetaGeneral>codigo}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
					<Text text="{listMdEsEtiquetaGeneral>descripcion}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
					<Text text="{listMdEsEtiquetaGeneral>cantidad}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
					<CheckBox selected="{listMdEsEtiquetaGeneral>conforme}" enabled="{=${asociarDatos>/estadoIdRmd_iMaestraId}=== 465 ? false : true}"/>
					<CheckBox selected="{listMdEsEtiquetaGeneral>procesoMenor}" enabled="{=${asociarDatos>/estadoIdRmd_iMaestraId}=== 465 ? false : true}"/>
					<OverflowToolbarButton tooltip="{i18n>accAddPasos}" text="{i18n>accAddPasos}" icon="sap-icon://edit" press="onEditMenuActionProcess"   type="Transparent"/>
				</ColumnListItem>
			</items>
		</Table>
		<endButton>
			<Button text="{i18n>btnClose}" press="onCancelEtiquetaResponsive" type="Transparent"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>