<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<Dialog title="{asociarDatos>/codigo} - {asociarDatos>/descripcion}" afterClose="onCancelFormulaResponsive">
		<Table width="auto" id="idTblInsumoFormula" sticky="HeaderToolbar,ColumnHeaders" mode="None" items="{aListInsumoAssignResponsive>/}" updateFinished="onUpdateFinish">
			<headerToolbar>
				<OverflowToolbar>
					<Title text="{i18n>tlInsumo} ({=${aListInsumoAssignResponsive>/}.length})"></Title>
					<ToolbarSpacer/>
					<ToolbarSeparator/>
					<OverflowToolbarButton text="{i18n>TbEPrint}" tooltip="{i18n>TbEPrint}" icon="sap-icon://print" press="onPrint" type="Transparent"/>
				</OverflowToolbar>
			</headerToolbar>
			<columns>
				<Column width="10%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbFCodInsumo}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="25%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbFDescripcion}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbFCantReceta}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="15%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbFCantRM}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
				<Column width="10%" hAlign="Left" vAlign="Middle" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
					<Text text="{i18n>TbFUM}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<customData>
						<core:CustomData key="mydata" value="{parts:[{path:'aListInsumoAssignResponsive>CompQty'},{path:'aListInsumoAssignResponsive>cantidadRm'}] , formatter:'.onFormaterBodyForm'}" writeToDom="true"></core:CustomData>
					</customData>
					<Text text="{aListInsumoAssignResponsive>Component}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
					<Text text="{aListInsumoAssignResponsive>Maktx} {aListInsumoAssignResponsive>Txtadic}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
					<Text text="{path: 'aListInsumoAssignResponsive>CompQty', formatter: '.formatter.formatDecimal'}" width="auto" wrapping="true" textAlign="End" textDirection="Inherit"/>
					<Text text="{path: 'aListInsumoAssignResponsive>cantidadRm', formatter: '.formatter.formatDecimal'}" width="auto" wrapping="true" textAlign="End" textDirection="Inherit"/>
					<Text text="{aListInsumoAssignResponsive>CompUnit}" width="auto" wrapping="true" textAlign="Begin" textDirection="Inherit"/>
				</ColumnListItem>
			</items>
		</Table>
		<endButton>
			<Button text="{i18n>btnClose}" press="onCancelFormulaResponsive" type="Transparent"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>