<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Popover showHeader="false" contentWidth="320px" placement="Left" initialFocus="action" ariaLabelledBy="master-title">
		<Table contextualWidth="Auto" items="{modelGeneral>/destinationsMD}">
			<headerToolbar>
				<OverflowToolbar>
					<core:Icon src="sap-icon://sales-document"/>
					<Title text="{i18n>tlDestinations}" level="H2"/>
					<ToolbarSpacer/>
				</OverflowToolbar>
			</headerToolbar>
			<columns>
				<Column >
					<Text text="{i18n>clNames}"/>
				</Column>
			</columns>
			<items>
				<ColumnListItem>
					<cells>
						<ObjectIdentifier title="{modelGeneral>usuario}"/>
					</cells>
				</ColumnListItem>
			</items>
		</Table>
	</Popover>
</core:FragmentDefinition>