<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog confirm="handleConfirmSettings">
		<sortItems>
			<ViewSettingsItem text="{i18n>fecha}" key="CreationDate"/>
			<ViewSettingsItem text="{i18n>tipo}" key="DocumentType" selected="true"/>
			<ViewSettingsItem text="{i18n>numero}" key="DocumentNr"/>
			<ViewSettingsItem text="{i18n>importe}" key="Amount"/>
		</sortItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>