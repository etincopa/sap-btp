<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.proyectoScp.controller.RegistroDeArmadores" xmlns:m="sap.m"
	xmlns:form="sap.ui.layout.form" xmlns:core="sap.ui.core">
	<m:Page id="registroDeArmadoresPage" showHeader="true" title="{=${Status}==='RECHAZADO' ? 'Reenvio' : 'Registro de Armadores' }"
		showFooter="true" showNavButton="false">
		<m:content>
			<form:Form width="100%" editable="true" visible="true">
				<form:title>
					<core:Title text="Datos de RUC" level="Auto"/>
				</form:title>
				<form:formContainers>
					<form:FormContainer visible="true">
						<form:formElements>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="Destino MP" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Select id="cmbDestinationMp" forceSelection="false" selectedKey="{armador>DestinationMp}" items="{ path: '/DestinationsMPSet' }"
										fieldGroupIds="mainGroup" width="120px" enabled="{=${Status}==='RECHAZADO' ? false : true }">
										<core:Item key="{DestinationMp}" text="{DestinationMp}"/>
									</m:Select>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="RUC Armador" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="rucArmador" fieldGroupIds="mainGroup" liveChange="onRucChange" value="{armador>Ruc}"
										placeholder="Nro RUC..." type="Text" showValueHelp="false" enabled="{=${Status}==='RECHAZADO' ? false : true }" visible="true" width="120px"
										valueHelpOnly="false" maxLength="11"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Button id="btnSunat" press="getRUC" text="Consultar SUNAT" textAlign="Begin" width="150px" target="_blank" wrapping="false"
										enabled="{=${Status}==='RECHAZADO' ? false : true }" visible="true" emphasized="true" subtle="false"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="Nombre" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="nombreArmador" fieldGroupIds="mainGroup" value="{armador>BusinessName}"
										placeholder="Nombre de Razón Social" type="Text" showValueHelp="false" enabled="{=${Status}==='RECHAZADO' ? false : true }" visible="true"
										width="auto" valueHelpOnly="false" maxLength="100"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="Dirección" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:TextArea id="direccion" value="{armador>Direccion}" placeholder="Ingresar dirección..." type="Text"
										showValueHelp="false" enabled="{=${Status}==='RECHAZADO' ? false : true }" visible="true" width="auto" valueHelpOnly="false" maxLength="200"
										fieldGroupIds="mainGroup"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="false">
								<form:label>
									<m:Label text="Padrón" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="padron" value="{armador>Padron}" placeholder="" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" maxLength="50"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="false">
								<form:label>
									<m:Label text="Distrito" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="distrito" value="{armador>Distric}" placeholder="" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" maxLength="35"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="false">
								<form:label>
									<m:Label text="Población" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="poblacion" value="{armador>Population}" placeholder="" type="Text" showValueHelp="false" enabled="true" visible="true"
										width="auto" valueHelpOnly="false" maxLength="35"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="false">
								<form:label>
									<m:Label text="Región" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="region" value="{armador>Region}" placeholder="" type="Text" showValueHelp="false" enabled="true" visible="true" width="auto"
										valueHelpOnly="false" maxLength="35"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="Rubro" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="rubro" fieldGroupIds="mainGroup" value="{armador>Industry}" placeholder="Ingresar Rubro de pesca..."
										type="Text" description="" showValueHelp="false" enabled="{=${Status}==='RECHAZADO' ? false : true }" visible="true" width="auto"
										valueHelpOnly="false" maxLength="50"/>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="true">
								<form:label>
									<m:Label text="Comp. Emitir" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Select id="cmbToEmitDoc" fieldGroupIds="mainGroup" items="{path:'/TpsEmitDocSet'}" editable="true"
										enabled="{=${Status}==='RECHAZADO' ? false : true }" forceSelection="false" visible="true" width="100%" valueState="None" maxWidth="100%"
										selectedKey="{armador>ToEmitDoc}">
										<m:items>
											<core:Item key="{ToEmitDoc}" text="{ToEmitDoc}" enabled="true"/>
										</m:items>
									</m:Select>
								</form:fields>
							</form:FormElement>
							<form:FormElement visible="false" id="frmCompPago">
								<form:label>
									<m:Label text="Comp. de pago" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
								</form:label>
								<form:fields>
									<m:Input id="idComprobante" value="{armador>Comprobante}" placeholder="" type="Text" description="" showValueHelp="false" enabled="false"
										visible="true" width="auto" valueHelpOnly="false" maxLength="200"/>
								</form:fields>
							</form:FormElement>
						</form:formElements>
						<form:title/>
					</form:FormContainer>
				</form:formContainers>
				<form:layout>
					<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
				</form:layout>
			</form:Form>
			<m:IconTabBar expandable="false" expanded="true" applyContentPadding="false" backgroundDesign="Solid" upperCase="false" visible="true"
				selectedKey="Ok">
				<m:items>
					<m:IconTabFilter icon="sap-icon://add-contact" iconColor="Positive" text="Datos Armador" count="" design="Horizontal" showAll="false"
						textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
						<m:content>
							<form:Form width="100%" editable="true" visible="true">
								<form:title>
									<core:Title text="Datos de Armador" level="Auto"/>
								</form:title>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Teléfono" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="telefono" fieldGroupIds="mainGroup" liveChange="onNumericLiveChange" value="{armador>Phone}"
														placeholder="Ingresar teléfono..." type="Text" showValueHelp="false" enabled="{=${Status}==='RECHAZADO' ? false : true }" visible="true"
														width="150px" valueHelpOnly="false" maxLength="12"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Celular" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="cellphone" fieldGroupIds="mainGroup" liveChange="onNumericLiveChange" value="{armador>CellPhone}"
														placeholder="Máx. 9 digitos" type="Text" showValueHelp="false" enabled="{=${Status}==='RECHAZADO' ? false : true }" visible="true"
														width="150px" valueHelpOnly="false" maxLength="9"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Moneda" required="true" design="Bold" width="100%" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<!--value="{path:'armador>Currency'}"-->
													<m:Select id="cmbCurrency" fieldGroupIds="mainGroup" items="{path:'/TpsCurrencySet'}" enabled="{=${Status}==='RECHAZADO' ? false : true }"
														forceSelection="false" visible="true" width="150px" valueState="None" maxWidth="100%" selectedKey="{armador>Currency}">
														<m:items>
															<core:Item key="{Currency}" text="{Currency}" enabled="true"/>
														</m:items>
													</m:Select>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="E-mail" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="emailArmador" placeholder="Email@gmail.com" type="Email" showValueHelp="false" enabled="true"
														visible="true" width="auto" valueHelpOnly="false" maxLength="0"/>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<m:Button id="btnEmail" press="onAddMail" text="Agregar Mail" type="Emphasized" icon="sap-icon://address-book" iconFirst="true" width="auto"
								enabled="true" visible="true" iconDensityAware="false"/>
							<m:Table id="tableCorreos" items="{path:'armador>ShipownerEmailSet'}" width="100%" noDataText="No hay datos" mode="None"
								showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true">
								<m:infoToolbar>
									<m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
										<m:content>
											<m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
										</m:content>
									</m:Toolbar>
								</m:infoToolbar>
								<m:headerToolbar/>
								<m:columns>
									<m:Column width="auto" hAlign="Left" vAlign="Top" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="@e-mail" width="auto" maxLines="2" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Eliminar" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem type="Active">
										<m:cells>
											<m:ObjectIdentifier title="" text="{armador>Mail}" titleActive="false" visible="true"/>
											<m:Button press="onDelete" icon="sap-icon://delete"/>
										</m:cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>
						</m:content>
					</m:IconTabFilter>
					<m:IconTabSeparator icon="" iconDensityAware="false"/>
					<m:IconTabFilter icon="sap-icon://capital-projects" iconColor="Neutral" text="Bancos" count="" design="Horizontal" showAll="false"
						textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
						<m:content>
							<form:Form width="100%" editable="true" visible="true">
								<form:title>
									<core:Title text="Datos de Bancos" level="Auto"/>
								</form:title>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Banco" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Select id="bancoArmador" items="{path:'/BanksSet'}" forceSelection="false" editable="true" enabled="true" visible="true"
														valueState="None" maxWidth="100%" change='onBankChange' width="270px">
														<m:items>
															<core:Item key="{BankId}" text="{BankName}" enabled="true"/>
														</m:items>
													</m:Select>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Tipo de cuenta" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Select id="tipoCuentaArmador" items="{path:'/TpsAccountSet'}" forceSelection="false" editable="true" enabled="true" visible="true"
														width="270px" valueState="None" maxWidth="100%" change='onBankChange'>
														<m:items>
															<core:Item key='{TpAccountDescp}' text="{TpAccount}" enabled="true"/>
														</m:items>
													</m:Select>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="N° de cuenta" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="cuentaArmador" liveChange="onNumericLiveChange" placeholder="Ingresar N° de cuenta..." type="Text" showValueHelp="false"
														enabled="true" visible="true" width="340px" valueHelpOnly="false" maxLength="18" description="          " />
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Moneda" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Select id="monedaArmador" items="{path:'/TpsCurrencySet'}" forceSelection="false" enabled="true" visible="true" width="160px"
														valueState="None">
														<m:items>
															<core:Item text="{Currency}" enabled="true"/>
														</m:items>
													</m:Select>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<m:Button id="btnBanco" press="onAddBanco" text="Agregar Banco" type="Emphasized" icon="sap-icon://official-service" iconFirst="true"
								width="auto" enabled="true" visible="true" iconDensityAware="false"/>
							<m:Table id="tableBanks" items="{path:'armador>ShipownerBanksSet'}" width="100%" noDataText="No hay datos" mode="None" showSeparators="All"
								growing="true" growingThreshold="20" growingScrollToLoad="true">
								<m:infoToolbar>
									<m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
										<m:content>
											<m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
										</m:content>
									</m:Toolbar>
								</m:infoToolbar>
								<m:headerToolbar/>
								<m:columns>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Banco" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="N° de cuenta" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Moneda" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Tipo de cuenta" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Eliminar" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem type="Active">
										<m:cells>
											<m:ObjectIdentifier title="" text="{armador>BankName}" titleActive="false" visible="true"/>
											<m:Text text="{armador>BankAccount}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
											<m:ObjectNumber number="" unit="{armador>Currency}" emphasized="true" state="None"/>
											<m:Text text="{armador>TpAccount}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
											<m:Button press="onDelete" icon="sap-icon://delete"/>
										</m:cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>
						</m:content>
					</m:IconTabFilter>
					<m:IconTabSeparator icon="" iconDensityAware="false"/>
					<m:IconTabFilter icon="sap-icon://citizen-connect" iconColor="Critical" text="Persona de conctacto" count="" design="Horizontal"
						showAll="false" textDirection="Inherit" enabled="true" visible="true" iconDensityAware="false">
						<m:content>
							<form:Form width="100%" editable="true" visible="true">
								<form:title>
									<core:Title text="Persona de contacto" level="Auto"/>
								</form:title>
								<form:formContainers>
									<form:FormContainer visible="true">
										<form:formElements>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Nombre" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="nombreContacto" liveChange="onTextInput" placeholder="Ingresar nombre" type="Text" showValueHelp="false" enabled="true"
														visible="true" width="auto" valueHelpOnly="false" maxLength="100"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Teléfono" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="telefonoContacto" liveChange="onNumericLiveChange" placeholder="Teléfono" type="Text" showValueHelp="false" enabled="true"
														visible="true" width="150px" valueHelpOnly="false" maxLength="12"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="Celular" design="Bold" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="celularContacto" liveChange="onNumericLiveChange" placeholder="Máx. 9 digitos" type="Text" showValueHelp="false" enabled="true"
														visible="true" width="120px" valueHelpOnly="false" maxLength="9"/>
												</form:fields>
											</form:FormElement>
											<form:FormElement visible="true">
												<form:label>
													<m:Label text="E-mail" design="Bold" width="100%" required="true" textAlign="Begin" textDirection="Inherit"/>
												</form:label>
												<form:fields>
													<m:Input id="emailContacto" placeholder="Ingresar e-mail" type="Email" showValueHelp="false" enabled="true" visible="true" width="auto"
														valueHelpOnly="false" maxLength="241"/>
												</form:fields>
											</form:FormElement>
										</form:formElements>
										<form:title/>
									</form:FormContainer>
								</form:formContainers>
								<form:layout>
									<form:ResponsiveGridLayout adjustLabelSpan="false" columnsL="2" labelSpanL="3" columnsM="1" labelSpanM="2" labelSpanS="12"/>
								</form:layout>
							</form:Form>
							<m:Button id="btnPerson" press="onAddContact" text="Agregar Contacto" type="Emphasized" icon="sap-icon://collaborate" iconFirst="true"
								width="auto" enabled="true" visible="true" iconDensityAware="false"/>
							<m:Table id="tableContacts" items="{path:'armador>ShipownerContactPersonsSet'}" width="100%" noDataText="No hay datos" mode="None"
								showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true">
								<m:infoToolbar>
									<m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
										<m:content>
											<m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
										</m:content>
									</m:Toolbar>
								</m:infoToolbar>
								<m:headerToolbar/>
								<m:columns>
									<m:Column width="auto" hAlign="Left" vAlign="Top" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Nombre" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Teléfono" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Celular" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="E-mail" width="auto" maxLines="2" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
									<m:Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
										<m:header>
											<m:Text text="Eliminar" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
										</m:header>
										<m:footer/>
									</m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem type="Active">
										<m:cells>
											<m:ObjectIdentifier title="" text="{armador>Name}" titleActive="false" visible="true"/>
											<m:Text text="{armador>Phone}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
											<m:Text text="{armador>CellPhone}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
											<m:Text text="{armador>Email}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
											<m:Button press="onDelete" icon="sap-icon://delete"/>
										</m:cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>
						</m:content>
					</m:IconTabFilter>
				</m:items>
				<m:content/>
			</m:IconTabBar>
		</m:content>
		<m:footer>
			<m:Bar design="Auto">
				<m:contentLeft>
					<m:Button id="btnAddEmb" text="Añadir Embarcación" type="Accept" icon="sap-icon://add" iconFirst="true" width="auto" enabled="true"
						visible="true" iconDensityAware="false" press="_onAgregarEmbarcacion"/>
					<m:Button id="btnValidate" text="{=${Status}==='RECHAZADO' ? 'Reenviar' : 'Solicitar la validación' }" type="Emphasized" iconFirst="true"
						width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onValidate"/>
				</m:contentLeft>
				<m:contentRight>
					<m:Button text="" type="Back" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="onNavBack"/>
				</m:contentRight>
			</m:Bar>
		</m:footer>
		<m:headerContent/>
		<m:subHeader/>
		<m:customHeader/>
	</m:Page>
</mvc:View>