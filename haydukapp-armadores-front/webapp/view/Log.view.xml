<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.proyectoScp.controller.Log" xmlns:m="sap.m">
    <m:Page showHeader="true" title="Log de armadores rechazados" showFooter="true" showNavButton="false">
        <m:content>
            <m:Table width="100%" noDataText="No hay datos" mode="None" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true">
                <m:infoToolbar>
                    <m:Toolbar width="100%" height="auto" design="Auto" visible="false" enabled="true">
                        <m:content>
                            <m:Label text="Label" design="Standard" width="100%" required="false" textAlign="Begin" textDirection="Inherit"/>
                        </m:content>
                    </m:Toolbar>
                </m:infoToolbar>
                <m:headerToolbar/>
                <m:columns>
                    <m:Column width="auto" hAlign="Left" vAlign="Top" popinDisplay="Inline" mergeDuplicates="false">
                        <m:header>
                            <m:Text text="Descripción" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </m:header>
                        <m:footer/>
                    </m:Column>
                </m:columns>
                <m:items>
                    <m:ColumnListItem type="Active">
                        <m:cells>
                            <m:ObjectIdentifier title="{Message}" text="" titleActive="false" visible="true"/>
                        </m:cells>
                    </m:ColumnListItem>
                </m:items>
            </m:Table>
        </m:content>
        <m:footer>
            <m:Bar design="Auto">
                <m:contentLeft>
                    <m:Button text="Editar" type="Accept" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onEditPress"/>
                    <m:Button text="Enviar para aprobación" type="Emphasized" iconFirst="true" width="auto" enabled="false" visible="false" iconDensityAware="false" press="_onApprovePress"/>
                </m:contentLeft>
                <m:contentMiddle/>
                <m:contentRight>
                    <m:Button text="" type="Back" iconFirst="true" width="auto" enabled="true" visible="true" iconDensityAware="false" press="_onNavBack"/>
                </m:contentRight>
            </m:Bar>
        </m:footer>
        <m:headerContent/>
        <m:subHeader/>
        <m:customHeader/>
    </m:Page>
</mvc:View>