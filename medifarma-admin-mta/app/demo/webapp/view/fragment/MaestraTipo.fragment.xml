<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns:core="sap.ui.core"
>
	<TableSelectDialog
		noDataText="{i18n>noDataText}"
		title=""
		confirmButtonText="Crear"
		search=".handleSearch"
		confirm=".onDialogClose"
		cancel=".onDialogClose"
		items="{
			path: 'MasterTypeListModel>/'
		}"
	>
		<ColumnListItem>
			<cells>
				<ObjectIdentifier
					title="{MasterTypeListModel>tabla}"
					text=""
				/>
				<Text text="{MasterTypeListModel>nombre}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Tipo"/>
				</header>
			</Column>
			<Column
				minScreenWidth="Tablet"
				demandPopin="true"
			>
				<header>
					<Text text="Nombre"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>