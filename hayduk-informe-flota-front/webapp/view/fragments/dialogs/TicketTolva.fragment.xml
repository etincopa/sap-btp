<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog type="Standard" showHeader="false" state="None" draggable="true" contentWidth="80%" class="sapUiSizeCompact">
		<customHeader>
			<Toolbar class="sapUiContentPadding">
				<Title text="Registrar Descarga" titleStyle="H5" class="sapUiSmallMarginBegin"/>
				<ToolbarSpacer/>
				<core:Icon src="sap-icon://decline" size="1.3rem" class="sapUiSmallMarginEnd setPointer" press=".descarga.closeTicketTolva"/>
			</Toolbar>
		</customHeader>
		<content>
			<Panel class="sapUiContentPadding">
				<Table id="tblTicketTolva" inset="false" items="{ path: '/results' }" mode="MultiSelect">
					<headerToolbar>
						<OverflowToolbar>
							<content>
								<Title text="Tickets" titleStyle="H4"/>
								<ToolbarSpacer/>
							</content>
						</OverflowToolbar>
					</headerToolbar>
					<columns>
						<Column>
							<Text text="Nro Ticket"/>
						</Column>
						<Column>
							<Text text="Fec. Ini. Desc."/>
						</Column>
						<Column>
							<Text text="H. Ini. Desc"/>
						</Column>
						<Column>
							<Text text="Fec. Fin. Desc."/>
						</Column>
						<Column>
							<Text text="H. Fin. Desc."/>
						</Column>
						<Column>
							<Text text="Peso acumulado"/>
						</Column>
						<Column>
							<Text text="Nro Tolva"/>
						</Column>
						<Column>
							<Text text="Nro Report"/>
						</Column>
					</columns>
					<items>
						<ColumnListItem >
							<cells>
								<Text text="{Tcket}"/>
								<Text text="{Fdesi}"/>
								<Text text="{Hdesi}"/>
								<Text text="{Fdesf}"/>
								<Text text="{Hdesf}"/>
								<Text text="{Pesoa}"/>
								<Text text="{Ntolv}"/>
								<Text text="{Nrepo}"/>
							</cells>
						</ColumnListItem>
					</items>
				</Table>
			</Panel>
		</content>
			<buttons>
			<Button type="Accept"  text="Aceptar" press=".descarga.registrarDescarga" />
			<Button type="Reject" text="Cancelar" press=".descarga.closeTicketTolva" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>