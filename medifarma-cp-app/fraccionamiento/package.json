{
  "name": "fraccionamiento-central-pesada",
  "productName": "Fraccionamiento",
  "version": "1.0.16",
  "description": "A minimal Electron application",
  "main": "./app/main.js",
  "buildDependenciesFromSource": true,
  "npmRebuild": false,
  "scripts": {
    "startui5": "ui5 serve",
    "start": "electron-forge start",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start-bat": "npm install && npm start",
    "build": "npx rimraf dist && copyfiles *.* */** dist/ && npx rimraf dist/node_modules && npx rimraf dist/server/db",
    "build:packager": "electron-packager . fraccionamiento-central-pesada --platform=darwin --arch=x64 --overwrite",
    "build:win": "electron-builder --win --dir",
    "dist:win": "electron-builder --win",
    "build:linux": "electron-builder --linux --dir",
    "dist:linux": "electron-builder --linux",
    "build:macos": "electron-builder --macos --dir",
    "dist:macos": "electron-builder --macos",
    "build:mac": "electron-builder --mac --dir",
    "dist:mac": "electron-builder --mac",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "license": "ISC",
  "build": {
    "appId": "com.medifarma.fraccionamientoappprd",
    "asar": false,
    "asarUnpack": [
      "**/*.node"
    ],
    "files": [
      "app/**/*"
    ],
    "copyright": "Â©2022 Medifarma, Inc.",
    "dmg": {
      "background": "buildApp/dmg-background.tiff",
      "icon": "buildApp/dmg-icon.icns",
      "window": {
        "width": "400",
        "height": "300"
      },
      "contents": [
        {
          "x": 100,
          "y": 100
        },
        {
          "x": 300,
          "y": 100,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "buildApp/icon.icns",
      "category": "public.app-category.utilities",
      "darkModeSupport": true,
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "hardenedRuntime": true,
      "entitlements": "buildApp/entitlements.mac.plist",
      "entitlementsInherit": "buildApp/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "win": {
      "icon": "buildApp/icon.ico",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "category": "Chat;GNOME;GTK;Network;InstantMessaging",
      "icon": "buildApp/icon.icns",
      "description": "Zulip Desktop Client for Linux",
      "target": [
        "AppImage",
        "rpm"
      ],
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    },
    "nsis": {
      "installerIcon": "buildApp/icon.ico",
      "uninstallerIcon": "buildApp/icon.ico",
      "uninstallDisplayName": "Fraccionamiendo Unistall",
      "license": "buildApp/license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@actions/core": "^1.2.4",
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@openui5/ts-types": "^1.60.5",
    "@ui5/cli": "^1.14.0",
    "config": "^3.3.1",
    "copyfiles": "latest",
    "electron": "^14.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "express": "^4.17.1",
    "http-proxy": "^1.18.0",
    "spsave": "^3.1.6"
  },
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "cli-truncate": "^3.1.0",
    "clone-response": "^1.0.2",
    "electron-root-path": "^1.0.16",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "nedb": "^1.8.0",
    "node-fetch": "^2.0.0",
    "odata": "^1.3.1",
    "simple-odata-server": "^1.1.2",
    "simple-odata-server-nedb": "^1.0.0"
  },
  "ui5": {
    "dependencies": [
      "@sap/ui5-builder-webide-extension",
      "ui5-task-stringreplacer"
    ]
  },
  "optionalDependencies": {
    "@sap/ui5-builder-webide-extension": "1.0.9",
    "ui5-task-stringreplacer": "*"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "fraccionamiento_central_pesada"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
