<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
>
	<TableSelectDialog
		id="idDlgMaterialEmbalaje"
		noDataText="Sin registros"
		title="Material Embalaje"
		search=".onSearchMaterial"
		confirm=".onSelectMaterial"
		cancel=".onCloseDialog"
		multiSelect="false"
		items="{
			path: 'MaterialEmbalajeListModel>/',
			sorter: {
				path: 'Matnr',
				descending: false
			}
		}"
	>
		<ColumnListItem vAlign="Middle">
			<cells>
				<ObjectIdentifier
					title="{MaterialEmbalajeListModel>Maktx}"
					text="{MaterialEmbalajeListModel>Matnr}"
				/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="30em">
				<header>
					<Text text="Embalaje"/>
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>